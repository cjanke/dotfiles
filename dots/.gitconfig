[user]
	name = Claire Janke
	email = claire.janke@shopify.com
[diff]
	algorithm = patience
[protocol]
	version = 2
[alias]
	co = checkout
	br = branch
	st = status
	ci = commit
	rpull = pull --rebase
	prom = pull --rebase origin master
	aa = add -A
	rc = rebase --continue
[core]
	editor = emacs
	commitGraph = true
[push]
	default = upstream
[gc]
	writeCommitGraph = true
[url "https://github.com/Shopify/"]
	insteadOf = git@github.com:Shopify/
	insteadOf = git@github.com:shopify/
	insteadOf = ssh://git@github.com/Shopify/
	insteadOf = ssh://git@github.com/shopify/
